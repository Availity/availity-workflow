{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "react-app:create",
      "program": "${workspaceFolder}/packages/workflow/index.js",
      "args": ["init", "my-proj", "--template", "https://github.com/Availity/availity-starter-complex.git", "-y"]
    },
    {
      "type": "node",
      "name": "react-app:start",
      "request": "launch",
      "cwd": "${workspaceFolder}/example",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/node_modules/.bin/av",
      "args": ["start"]
    },
    {
      "type": "node",
      "name": "react-app:build",
      "request": "launch",
      "cwd": "${workspaceFolder}/example",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/node_modules/.bin/av",
      "args": ["build"]
    },
    {
      "type": "node",
      "name": "react-app:test",
      "request": "launch",
      "cwd": "${workspaceFolder}/example",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/node_modules/.bin/av",
      "args": ["test"]
    },
    {
      "type": "node",
      "name": "docs:start",
      "request": "launch",
      "cwd": "${workspaceFolder}/docs",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "program": "${workspaceFolder}/node_modules/.bin/gatsby",
      "args": ["develop"]
    }
  ]
}
