{
  "targetDefaults": {
    "build": {
      "cache": true,
      "dependsOn": [
        {
          "target": "build",
          "dependencies": true
        }
      ]
    },
    "test": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "package": {
      "cache": true,
      "dependsOn": [
        {
          "target": "package",
          "dependencies": true
        }
      ]
    },
    "prepare": {
      "cache": true,
      "dependsOn": [
        {
          "target": "prepare",
          "dependencies": true
        }
      ]
    }
  },
  "extends": "@nx/workspace/presets/npm.json",
  "useInferencePlugins": false,
  "defaultBase": "master",
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": true
    }
  }
}
