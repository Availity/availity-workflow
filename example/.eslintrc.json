{
  "extends": "availity/browser",
  "rules": {
    "import/extensions": 0
  },
  "settings": {
    "import/resolver": {
      "typescript": {},
      "root-import": {
        "rootPathPrefix": "@",
        "rootPathSuffix": "project/app",
        "extensions": [".js", ".jsx", ".tsx", ".ts"]
      }
    }
  }
}
